<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='resources/feed.xsl'?>
<interface uri="https://limaono.github.io/0cuis.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>0cuis</name>
  <summary>Launch (and prepare if needed) a Cuis Smalltalk image</summary>

  <description>
  I know how to start a Cuis image, and to prepare it if necessary if I am given
  access to a copy of the Cuis Smalltalk distribution
  </description>

  <homepage>https://github.com/LimaOno/CuisCommand</homepage>

  <feed-for interface="https://limaono.github.io/0cuis.xml"/>

  <group license="OSI Approved :: MIT/X">
    <command name="run" path="0cuis"/>

    <requires interface="https://limaono.github.io/OpenSmalltalk.xml">
      <environment insert="." mode="append" name="PATH"/>
    </requires>
    <requires interface="https://limaono.github.io/CuisSource.xml">
      <environment mode="replace" name="CUISPATH" value="."/>
    </requires>

    <implementation arch="*-*" id="sha1new=278849c8247f4890929229f18460285556cd54cc" released="2019-09-06" version="0.1-pre1">
      <manifest-digest sha256new="JIYVOT4I62KPZVF577JV3GDX4FQNWDYOWXVLJZUYBMZO52FSRQMQ"/>
      <archive extract="0cuis-0.1-pre1" href="https://github.com/LimaOno/0cuis/archive/v0.1-pre1.zip" size="2861"/>
    </implementation>
    <implementation arch="*-*" id="sha1new=46adf0f33a8220b26e3da54608bbc4689e6143cf" released="2019-10-08" version="0.1-pre3">
      <manifest-digest sha256new="DPQ5PRSGWIFPPOPQBYW2BJNWAYXA4RRYUDI6FQYW2IWQKFQ5DLDQ"/>
      <archive extract="0cuis-0.1-pre3" href="https://github.com/LimaOno/0cuis/archive/v0.1-pre3.zip" size="3617"/>
    </implementation>
    <implementation arch="*-*" id="sha1new=62559cbd218d508fbfa4125e22e17adec58a7fba" released="2019-10-12" version="0.2">
      <manifest-digest sha256new="4ELQKARDRWBNNQWQ2MRP3XAAIOFJLFLY54YC2BZFYGI6EBUOLV4A"/>
      <archive extract="0cuis-0.2" href="https://github.com/LimaOno/0cuis/archive/v0.2.zip" size="3584"/>
    </implementation>
    <group>
      <requires interface="http://repo.roscidus.com/python/python">
        <version after="3.5"/>
      </requires>
      <command name="run" path="0cuis">
        <runner interface="http://repo.roscidus.com/python/python"/>
      </command>
      <implementation arch="*-*" id="sha1new=e12a20b4e08ba016a6804bd1e6386ddcb2570044" released="2019-10-16" version="0.3">
        <manifest-digest sha256new="NERPWGF5DWXU6L75SFXVECGQYYVKUX5VLG6474TGF2FXAYTF7KYQ"/>
        <archive extract="0cuis-0.3" href="https://github.com/LimaOno/0cuis/archive/v0.3.zip" size="4346"/>
      </implementation>
    </group>
  </group>
</interface>
